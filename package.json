{
  "name": "jobslave",
  "version": "1.0.0",
  "private": true,
  "description": "Automated job application SaaS with LLM-powered form filling",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:web": "turbo dev --filter=@jobslave/web",
    "dev:api": "turbo dev --filter=@jobslave/api",
    "dev:desktop": "turbo dev --filter=@jobslave/desktop",
    "build:desktop": "turbo build --filter=@jobslave/desktop"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.0",
    "@types/node": "^22.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
